{
  "version": 2,
  "name": "bloodbank",
  "builds": [
    { "src": "index.js", "use": "@vercel/node" }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.js"
    },
    {
      "src":"/favicon.ico",
      "headers":{
        "Cache-Control":"public, max-age=3600"
      },
      "continue":true
    },
    {
      "src":"/js/(.*)",
      "headers":{
        "Cache-Control":"public, max-age=3600, immutable"
      },
      "continue":true
    },
    {
      "src":"/img/(.*)",
      "headers":{
        "Cache-Control":"public, max-age=3600, immutable"
      },
      "continue":true
    },
    {
      "src":"/css/(.*)",
      "headers":{
        "Cache-Control":"public, max-age=3600, immutable"
      },
      "continue":true
    }
  ]
}
